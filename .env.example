# JuiceBox Example Environment Variables
# Copy this file to .env and edit as needed

# Maximum file size per upload (e.g. 500MB, 1GB, 104857600 for 100MB)
MAX_FILE_SIZE=500MB

# App environment (set to 'production' for prod)
APP_ENV=development

# Mailgun (optional, for report emails)
MAILGUN_API_KEY=
MAILGUN_DOMAIN=
REPORT_EMAIL_TO=
REPORT_EMAIL_FROM=
